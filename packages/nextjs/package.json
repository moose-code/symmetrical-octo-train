{
  "name": "@se-2/nextjs",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "gql:codegen": "graphql-codegen",
    "gql:watch": "graphql-codegen -w",
    "next:dev": "next dev",
    "next:start": "pnpm dev",
    "dev": "concurrently -n gql:codegen,next:dev 'pnpm gql:watch' 'pnpm next:dev'",
    "start": "pnpm dev",
    "prestart": "pnpm gql:codegen",
    "build": "next build",
    "serve": "next start",
    "lint": "next lint",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "check-types": "tsc --noEmit --incremental",
    "vercel": "vercel",
    "vercel:yolo": "vercel --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@apollo/experimental-nextjs-app-support": "^0.4.3",
    "@ethersproject/providers": "^5.7.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@heroicons/react": "^2.0.11",
    "@rainbow-me/rainbowkit": "1.0.8",
    "@uniswap/sdk-core": "^4.0.1",
    "@uniswap/v2-sdk": "^3.0.1",
    "abitype": "^0.9.8",
    "blo": "^1.0.1",
    "daisyui": "^3.5.1",
    "graphql": "^16.8.1",
    "next": "^13.1.6",
    "nextjs-progressbar": "^0.0.16",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-hot-toast": "^2.4.0",
    "use-debounce": "^8.0.4",
    "usehooks-ts": "^2.7.2",
    "viem": "^1.6.7",
    "wagmi": "1.3.10",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@parcel/watcher": "^2.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^8.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.15.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.16",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vercel": "^28.15.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "usehooks-ts@2.7.2": "patches/usehooks-ts@2.7.2.patch"
    }
  }
}
